<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ct.daan.recordingyourlife">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Note.NoteActivity"
            android:label="@string/note"/>
        <activity android:name=".Note.NotePageActivity"
            android:label="@string/note"/>
        <activity android:name=".Note.NewNotePageActivity"
            android:label="@string/note"/>
        <activity android:name=".Note.RemindersActivityByNew"
            android:label="@string/note"/>
        <activity android:name=".Note.AddReminderActivityByNew"
            android:label="@string/note"/>
        <activity android:name=".Note.AddReminderActivity"
            android:label="@string/note"/>
        <activity android:name=".Note.ReminderActivity"
            android:label="@string/note"/>
        <activity android:name=".Note.RemindersActivity"
            android:label="@string/note"/>
        <activity android:name=".Schedule.NewSchedulePageActivity"
            android:label="@string/schedule"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Schedule.NewSchedulePageActivity" />
        </activity>
        <activity android:name=".Event.AddEventActivity"
            android:label="@string/event"/>
        <activity android:name=".Event.ChangEventActivity"
            android:label="@string/event"/>
        <activity android:name=".Event.EventActivity"
            android:label="@string/event"/>
        <activity android:name=".Diary.DiaryActivity"
            android:label="@string/diary"/>
        <activity android:name=".Diary.DiaryPageActivity"
            android:label="@string/diary"/>
        <activity android:name=".Diary.AddDiaryPageActivity"
            android:label="@string/diary"/>
        <activity android:name=".Exam.ExamActivity"
            android:label="@string/exam"/>
        <activity android:name=".Exam.ExamPageActivity"
            android:label="@string/exam"/>
        <activity android:name=".Exam.AddExamPageActivity"
            android:label="@string/exam"/>
        <activity android:name=".Table.AddSubjectTableActivity"
            android:label="@string/table"
            android:parentActivityName=".Table.TableActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Table.AddSubjectTableActivity" />
        </activity>
        <activity android:name=".Table.AddTableSettingsActivity"
            android:label="@string/table"
            android:parentActivityName=".Table.TableActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Table.AddSubjectTableActivity" />
        </activity>
        <activity android:name=".Table.TableActivity"
            android:label="@string/table"/>
        <activity android:name=".Table.TableSettingsActivity"
            android:label="@string/table"/>

        <receiver android:name=".Reminder.AlarmReceiver">
            <intent-filter>
                <action android:name="net.macdidi.broadcast01.action.MYBROADCAST01" />
            </intent-filter>
        </receiver>
    </application>

</manifest>